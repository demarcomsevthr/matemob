<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
       xmlns:g="urn:import:com.google.gwt.user.client.ui" 
       xmlns:gc="urn:import:it.mate.gwtcommons.client.ui" 
       ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
       ui:generateLocales="default" xmlns:o="urn:import:it.mate.onscommons.client.ui">
       
  <g:HTMLPanel ui:field="wrapperPanel" addStyleNames="app-page-wrapper-panel">
  
    <o:OnsPage>
    
      <o:OnsToolbar>
        <div class="left">
          <o:OnsBackButton addStyleNames="ons-back-button-realign"/>
        </div>
        <div class="center">Preview Timbro</div>
      </o:OnsToolbar>
      
      <g:HTML addStyleNames="app-item-preview" ui:field="previewPanel">
        <i>Preview del timbro</i>
      </g:HTML>

      <o:OnsLabel ui:field="lbNome"/> 
      
      <o:OnsLabel ui:field="lbDimensioni"/> 
      
      <!-- animation="fadeIn" -->
      <o:OnsList addStyleNames="app-timbri-bottom-panel ons-fadein" modifier="noborder" ui:field="list" />
      
    </o:OnsPage>
    
  </g:HTMLPanel>
  
</ui:UiBinder>