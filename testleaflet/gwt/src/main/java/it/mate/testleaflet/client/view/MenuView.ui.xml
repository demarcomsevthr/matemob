<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
       xmlns:g="urn:import:com.google.gwt.user.client.ui" 
       xmlns:gc="urn:import:it.mate.gwtcommons.client.ui" 
       ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
       ui:generateLocales="default" xmlns:o="urn:import:it.mate.onscommons.client.ui">
       
  <g:HTMLPanel ui:field="wrapperPanel" addStyleNames="app-page-wrapper-panel">

    <o:OnsPage>
    
      <o:OnsToolbar addStyleNames="app-menu-toolbar-panel">
        <div class="center">Timbri Copycart</div>
      </o:OnsToolbar>
      
      <o:OnsList addStyleNames="app-menu-list">
        <o:OnsListItem ui:field="btnTimbri" modifier="chevron" addStyleNames="app-menu-item-timbri">
          <table><tr><td><span class="app-stamp-img"></span></td> <td><span class="app-menu-text">Timbri</span></td> </tr></table>
        </o:OnsListItem>
        <o:OnsListItem ui:field="btnCart" modifier="chevron">
          <table><tr><td><span class="fa fa-shopping-cart"></span></td> <td><span class="app-menu-text">Carrello</span></td> </tr></table>
        </o:OnsListItem>
        <o:OnsListItem ui:field="btnOrdini" modifier="chevron">
          <table><tr><td><span class="fa fa-credit-card"></span></td> <td><span class="app-menu-text">I tuoi ordini</span></td> </tr></table>
        </o:OnsListItem>
        <o:OnsListItem ui:field="btnMessaggi" modifier="chevron">
          <table><tr><td><span class="fa fa-comment"></span></td> <td><span class="app-menu-text">Messaggi</span></td> </tr></table>
        </o:OnsListItem>
        <o:OnsListItem ui:field="btnSettings" modifier="chevron">
          <table><tr><td><span class="fa fa-cog"></span></td> <td><span class="app-menu-text">Opzioni</span></td> </tr></table>
        </o:OnsListItem>
      </o:OnsList>
      
    </o:OnsPage>
    
  </g:HTMLPanel>
  
</ui:UiBinder>