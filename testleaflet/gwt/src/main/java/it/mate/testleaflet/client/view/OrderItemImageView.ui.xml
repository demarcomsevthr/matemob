<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
       xmlns:g="urn:import:com.google.gwt.user.client.ui" 
       xmlns:gc="urn:import:it.mate.gwtcommons.client.ui" 
       ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
       ui:generateLocales="default" xmlns:o="urn:import:it.mate.onscommons.client.ui">
       
  <g:HTMLPanel ui:field="wrapperPanel" addStyleNames="app-page-wrapper-panel">
  
    <o:OnsPage>
    
      <o:OnsToolbar>
        <div class="left">
          <o:OnsBackButton/>
        </div>
        <div class="center">Immagine Timbro</div>
      </o:OnsToolbar>
      
      <g:HTML ui:field="imgHtml"/>
      
      <o:OnsLabel ui:field="lbNome"/> 
      
      <o:OnsList addStyleNames="app-timbri-bottom-panel" modifier="noborder" ui:field="bottomList">
        <o:OnsButton text="Inserisci immagine" ui:field="btnAdd"/>  
      </o:OnsList>
      
    </o:OnsPage>
    
  </g:HTMLPanel>
  
</ui:UiBinder>