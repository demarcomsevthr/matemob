<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
       xmlns:g="urn:import:com.google.gwt.user.client.ui" 
       xmlns:gc="urn:import:it.mate.gwtcommons.client.ui" 
       ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
       ui:generateLocales="default" xmlns:o="urn:import:it.mate.onscommons.client.ui">
       
  <g:HTMLPanel ui:field="wrapperPanel" addStyleNames="app-page-wrapper-panel">
  
    
  
    <o:OnsPage addStyleNames="app-timbri-page">
    
      <o:OnsToolbar>
        <div class="left">
          <o:OnsBackButton addStyleNames="ons-back-button-realign"/>
        </div>
        <div class="center" id="onsUidToolbarCenterDiv">Elenco Timbri</div>
      </o:OnsToolbar>
      
      <o:OnsCarousel addStyleNames="app-timbri-carousel ons-fadein" autoscroll="true" ui:field="carousel" swipeable="true" autoscrollRatio="0.2"/>

      <o:OnsButton text="Ordina il timbro" ui:field="btnBuy" addStyleNames="app-timbri-choose-btn"/>
      
    </o:OnsPage>
    
  </g:HTMLPanel>
  
</ui:UiBinder>