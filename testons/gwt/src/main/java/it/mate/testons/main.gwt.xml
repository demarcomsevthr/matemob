<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.3//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.3/distro-source/core/src/gwt-module.dtd">
<module rename-to="main">

  <inherits name="com.google.gwt.user.User" />
  
  <inherits name="com.google.gwt.inject.Inject"/>
  
  <inherits name="com.google.gwt.logging.Logging" />
  <define-property name="gwt.logging.popupHandler" values="ENABLED, DISABLED" />
  <!-- 
  <set-property name="gwt.logging.simpleRemoteHandler" value="DISABLED" />
  <set-property name="gwt.logging.popupHandler" value="DISABLED" />
   -->
  
  <!-- PER INIZIALIZZAZIONE TIMBRI IN DESKTOP -->
  <inherits name="com.google.gwt.http.HTTP" />  
  
  <inherits name='com.googlecode.gwtphonegap.PhoneGap' />
  
  <set-property name="user.agent" value="safari" />

  <add-linker name="xsiframe" />
  
  <set-property name="compiler.stackMode" value="strip"/>

  <inherits name="it.mate.gwtcommons.Commons" />
  
  <inherits name="it.mate.onscommons.onscommons" />
  
  <inherits name="it.mate.testons.Adapter" />
  
  <replace-with class="it.mate.testons.client.factories.AppClientFactoryImpl">
    <when-type-is class="it.mate.testons.client.factories.AppClientFactory"/>
  </replace-with>

  <entry-point class="it.mate.testons.client.MainEntryPoint" />
  
  <extend-property name="locale" values="it"/>

  <source path="client" />
  <source path="shared" />
  
  <public path="public" />

  <collapse-all-properties />
  
</module>
